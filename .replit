modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["libxcrypt", "libyaml", "postgresql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dashboard"

[[workflows.workflow]]
name = "dashboard"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python services/dashboard/main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8000
externalPort = 8000

[[ports]]
localPort = 8002
externalPort = 3003

[[ports]]
localPort = 8003
externalPort = 3002

[[ports]]
localPort = 8004
externalPort = 3001

[[ports]]
localPort = 8007
externalPort = 3000
