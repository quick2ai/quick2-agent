# Database Configuration
PGUSER=quick2user
PGPASSWORD=quick2pass
PGDATABASE=quick2agent
PGHOST=postgres
PGPORT=5432
DATABASE_URL=postgresql://quick2user:quick2pass@postgres:5432/quick2agent

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# MinIO Configuration
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET=quick2-artifacts

# Service URLs (for local development)
GATEWAY_URL=http://localhost:8000
PLANNER_URL=http://localhost:8001
ROUTER_URL=http://localhost:8002
EXECUTOR_URL=http://localhost:8003
VALIDATOR_URL=http://localhost:8004
MEMORY_URL=http://localhost:8005
BENCHMARKS_URL=http://localhost:8006
DASHBOARD_URL=http://localhost:8007

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=

# Session Secret
SESSION_SECRET=your-secret-key-here-change-in-production

# Autonomy Settings
DEFAULT_AUTONOMY_MODE=collaborator
ENABLE_AUTO_APPROVAL=false

# Rate Limiting
MAX_TASKS_PER_MINUTE=100
MAX_CONCURRENT_TASKS=50
MAX_COST_PER_HOUR=100.0

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
